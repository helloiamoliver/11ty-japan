    {% for entry in entries %}
            {% if entry.post_list %}
                {% set posts = collections[entry.post_list] %}
                {% if posts %}
                    <ul>
                        {% for post in posts | reverse %}
                            <li>
                                <time datetime="{{ post.date }}">{{ post.date | formatDate }}</time>
                                <a href="{{ post.url | url }}">{{ post.data.title }}</a>
                            </li>
                        {% endfor %}
                    </ul>
                {% endif %}
            {% endif %}
        </li>
    {% endfor %}